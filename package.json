{
  "name": "@unifygtm/intent-client",
  "version": "0.1.3",
  "license": "UNLICENSED",
  "private": false,
  "main": "./dist/minjs/intent-client.js",
  "types": "./dist/types/client/index.d.ts",
  "nodemonConfig": {
    "watch": [
      "./src/"
    ],
    "ext": "ts,json"
  },
  "scripts": {
    "build:client": "pnpm generate && ts-node src/build-client.ts",
    "build:browser": "pnpm generate && ts-node src/build-browser.ts",
    "build:browser:s3:prod": "pnpm build:browser && aws s3 cp ./dist/minjs/unify-tag-script.js s3://unifygtm-public/tag/v1/script.js",
    "build:browser:s3:staging": "pnpm build:browser && aws s3 cp ./dist/minjs/unify-tag-script.js s3://unifygtm-public/tag/v1/script-staging.js",
    "build:browser:s3:testing": "pnpm build:browser && aws s3 cp ./dist/minjs/unify-tag-script.js s3://unifygtm-public/tag/v1/script-testing.js",
    "generate": "pnpm openapi-typescript '../unify/api/tsp-output/@typespec/openapi3/openapi.UnifyAnalyticsApi.yaml' -o './src/spec.d.ts'",
    "start": "http-server -o -p 0",
    "test": "pnpm jest"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@faker-js/faker": "^8.4.1",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/jest": "^29.5.5",
    "@types/js-cookie": "^3.0.3",
    "@types/node": "^18.17.1",
    "@types/uuid": "^9.0.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^29.7.0",
    "esbuild": "^0.17.19",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-localstorage-mock": "^2.4.26",
    "jest-mock": "^29.7.0",
    "jest-mock-extended": "^3.0.7",
    "openapi-typescript": "^6.7.6",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "user-agent-data-types": "^0.4.2"
  },
  "dependencies": {
    "js-base64": "^3.7.5",
    "js-cookie": "^3.0.5",
    "uuid": "^9.0.0"
  }
}
